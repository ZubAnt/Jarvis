CC = g++

C14 = -std=c++14

BINCLUDE = -I/usr/include

BLIB = -L/usr/lib

LIBS = -lboost_system-mt

LCURL = -lcurl

LPROTOBUF = -lprotobuf

SOURCE = main.cpp

OSOURCE = main.o

CFLAG = -c

PROTOC = protoc

CPP_OUT = --cpp_out=.

BASICPB = CompilePB/basic.pb.cc

VOICEPROXYPB = CompilePB/voiceproxy.pb.cc

all: main
	rm -rf *.o

main: $(OSOURCE)
	$(CC) $(C14) $(BINCLUDE) $(BLIB) $(LIBS) $(OSOURCE) $(LCURL) $(LPROTOBUF) $(BASICPB) $(VOICEPROXYPB) -o test

$(OSOURCE):
	$(CC) $(C14) $(CFLAG) $(SOURCE)

clean:
	rm -rf test
